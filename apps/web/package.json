{
  "name": "@communitii/web-ssr",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "install": "yarn build-all",
    "next": "yarn build-firebase && cd \"src/client\" && yarn && yarn dev",
    "preserve": "yarn build-all",
    "serve": "firebase serve",
    "predeploy": "yarn build-all",
    "deploy": "firebase deploy",
    "build-all": "yarn build-next && yarn build-firebase",
    "build-next": "cd \"src/client\" && yarn && yarn build",
    "build-firebase": "cd \"src/functions\" && yarn"
  },
  "devDependencies": {
    "firebase-tools": "^3.17.4"
  }
}
